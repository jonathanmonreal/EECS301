module dff3x2(clk, reset, input_a, input_b, a_dff, b_dff);

	wire [2:0] a_dff, b_dff;

	flipflop a0(
		.clk(clk),
		.reset(reset),
		.d(input_a),
		.q(a_dff[0])
	);

	flipflop a1(
		.clk(clk),
		.reset(reset),
		.d(a_dff[0]),
		.q(a_dff[1])
	);

	flipflop a2(
		.clk(clk),
		.reset(reset),
		.d(a_dff[1]),
		.q(a_dff[2])
	);

	flipflop b0(
		.clk(clk),
		.reset(reset),
		.d(input_b),
		.q(a_dff[0])
	);

	flipflop b1(
		.clk(clk),
		.reset(reset),
		.d(input_a),
		.q(a_dff[0])
	);

	flipflop b2(
		.clk(clk),
		.reset(reset),
		.d(input_a),
		.q(a_dff[0])
	);

endmodule